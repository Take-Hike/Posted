        maponeset.holdfor["N"] = q
    end, {min = 0.1, max = 5, default = 0.1})

    spawn(function()
        while true do wait()
            if maponeset.autoskillsenabled then
                for i,v in pairs(maponeset.autoskills) do
                    if v == "Z" then
                        keypress(0x5A)
                        wait(maponeset.holdfor["Z"])
                        keyrelease(0x5A)
                    elseif v == "X" then
